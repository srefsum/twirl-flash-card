                     %= form_for '/Scores' => {format => 'txt'} => (method => 'POST') => begin
                        <%= submit_button "$SubmitButtLabl", id => 'Scores', onclick => "doFunction();" %>
                        <%=  hidden_field Skjema => '/submit', id => 'Skjema'  %>  
